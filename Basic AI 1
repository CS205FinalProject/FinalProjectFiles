   public void discardAI()
   {
      if discard.isEmpty() = false;
      {
         discardTop = compareCard(discard.getTopCard());
      
         if discardTop = card1
         {
            card placeHolder = discard.getTopcard();
            discard.setTopCard(hand.getCard(1))
            card1 = placeHolder;
            card1Known = true;
            hasPlayed = true;
         }
         else if discardTop = card2
         {
            card2 = discardTop;
            card2Known = true;
            hasPlayed = true;
         }
         else if discardTop = card3
         {
            card3 = discardTop;
            card3Known = true;
            hasPlayed = true;
         }
         else if discardTop = card4
         {
            card4 = discardTop;
            card4Known = true;
            hasPlayed = true;
         }
         else if (discard.getTop < some###  && numUnknowns > 0)
         {
            for (i=0; i<3; i++)
            {
               if ((hand.getCard(i).isUnknown() = true) or(hand.getCard(i).isPowerCard() = true)
               {
                  card placeHolder = discard.getTopcard();
                  discard.setTopCard(hand.getCard(i));
                  hand.setCard(i) = placeHolder;
                  i = 3;
                  hasPlayed = true;
                  
               }
            }
         }   
      
   }  
   
